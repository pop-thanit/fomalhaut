---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Thanks from "@components/infopages/ThankYou.astro";
import { type AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Thank You - sujitra Studio: Email Successfully Sent",
	description:
		"Thank you for reaching out to sujitra Studio! Your email has been successfully sent. We appreciate your interest and will respond promptly.",
	canonical: "https://sujitra.vercel.app//it/thank-you/",
};
---

<BaseLayout seo={seo}>
	<Thanks />
</BaseLayout>
