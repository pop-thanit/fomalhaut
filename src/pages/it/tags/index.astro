---
import { getCollection } from "astro:content";
import BaseLayout from "@layouts/BaseLayout.astro";
import Tags from "@components/blog/Tags.astro";
import { type AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Tags - Explore Topics at sujitra Studio",
	description:
		"Browse through the diverse range of tags at sujitra Studio to find articles and insights tailored to your interests in web design and SEO.",
	canonical: "https://sujitra.vercel.app//it/tags/",
};
---

<BaseLayout seo={seo}>
	<Tags />
</BaseLayout>
