---
import BaseLayout from "@/layouts/BaseLayout.astro";
import PrivacyPage from "@/components/infopages/Privacy.astro";
import { type AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Privacy Policy - sujitra Studio: Your Privacy Matters",
	description:
		"Our Privacy Policy outlines how sujitra Studio protects your personal information. Discover our commitment to your privacy and data security.",
	canonical: "https://sujitra.vercel.app//privacy/",
};
---

<BaseLayout seo={seo}>
	<PrivacyPage />
</BaseLayout>
